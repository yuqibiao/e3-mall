<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yyyu.manager.dao.TempTagMapper">
    <resultMap id="BaseResultMap" type="com.yyyu.manager.pojo.TempTag">
        <id column="tag_id" jdbcType="BIGINT" property="tagId"/>
        <id column="tag_epc" jdbcType="VARCHAR" property="tagEpc"/>
        <id column="tag_type" jdbcType="INTEGER" property="tagType"/>
        <id column="add_time" jdbcType="TIMESTAMP" property="addTime"/>
    </resultMap>

    <select id="addTag" parameterType="java.util.List" >
        INSERT INTO tmp_tag(tag_epc, tag_type, add_time)
        VALUES
        <foreach collection="list" item="item" index="index" separator="," >
            (#{item.tagEpc} , #{item.tagType} , #{item.addTime})
        </foreach>
    </select>

</mapper>